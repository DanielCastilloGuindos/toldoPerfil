{
	"version": 3,
	"routes": [
		{
			"handle": "filesystem"
		},
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"src": "^/_server-islands/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/_image/?$",
			"dest": "_render"
		},
		{
			"src": "^/404/?$",
			"dest": "_render"
		},
		{
			"src": "^/admin/create/?$",
			"dest": "_render"
		},
		{
			"src": "^/admin/edit/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/admin/edit/?$",
			"dest": "_render"
		},
		{
			"src": "^/admin/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/analytics/track/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/auth/login/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/auth/logout/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/customers/delete/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/customers/insert\\.json/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/customers/select\\.json/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/customers/toggle-visibility/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/sendEmail\\.json/?$",
			"dest": "_render"
		},
		{
			"src": "^/legal-advice/?$",
			"dest": "_render"
		},
		{
			"src": "^/privacy-policy/?$",
			"dest": "_render"
		},
		{
			"src": "^/products/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/products\\.json/?$",
			"dest": "_render"
		},
		{
			"src": "^/productsDB/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/$",
			"dest": "_render"
		},
		{
			"src": "^/.*$",
			"dest": "_render",
			"status": 404
		}
	]
}